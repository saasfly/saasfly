{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "paths": {
      "#imports": ["./nitro-imports"],
      "~/*": ["../../*"],
      "@/*": ["../../*"],
      "~~/*": ["../../*"],
      "@@/*": ["../../*"]
    }
  },
  "include": ["./nitro.d.ts", "../../**/*"]
}
